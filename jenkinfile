pipeline {
	  agent any
	  stages {
	    stage('Clone sources') {
	           // git url: 'https://github.com/VijayaLakshmiSaggam/samplejavaapp'
		    steps {
                        sh '''
                        cd $WORKSPACE
                        git init
                        git remote remove origin
                        git remote add origin http://vijayalakshmisaggam:21Oct@1996@https://github.com/VijayaLakshmiSaggam/samplejavaapp.git
                        git pull origin master --allow-unrelated-histories
                        '''
                          }

	                           }
	    stage('Compile') {
	      steps {
	        sh script: 'mvn clean compile'
	            }
	                     }
	    stage('Test') {
	      steps {
	        sh script: 'mvn test'
	            }
	                     }
	    stage('Package') {
	      steps {
	        sh script: 'mvn package'
	            }
	                     }
	         }
	         }
